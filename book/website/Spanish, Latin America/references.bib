@article{Ioannidis2005False,
    doi = {10.1371/journal.pmed.0020124},
    autor = {Ioannidis, John P. A.},
    diario = {PLOS Medicine},
    publicador = {Public Library of Science},
    título = {Why Most Published Research Findings Are False},
    año = {2005},
    mes = {08},
    volumen = {2},
    url = {https://doi.org/10.1371/journal.pmed.0020124},
    páginas = {null},
    abstracto = {Summary There is increasing concern that most current published research findings are false. La probabilidad de que una afirmación de una investigación sea cierta puede depender del poder y sesgo del estudio, el número de otros estudios sobre la misma cuestión, y, lo que es más importante, la relación de verdad a ninguna relación entre las relaciones probadas en cada campo científico. En este marco, es menos probable que un hallazgo de investigación sea cierto cuando los estudios realizados en un campo son más pequeños; cuando los tamaños del efecto son más pequeños; cuando hay un mayor número y menor preselección de relaciones probadas; donde hay una mayor flexibilidad en los diseños, definiciones, resultados y modos analíticos; cuando hay mayor interés financiero y de otro tipo y prejuicio; y cuando más equipos están involucrados en un campo científico en busca de importancia estadística. Las simulaciones muestran que para la mayoría de los diseños y entornos de estudio, es más probable que una investigación diga que es falsa que verdadera. Además, para muchos campos científicos actuales, los hallazgos de investigación afirmados pueden ser a menudo simples medidas precisas del sesgo predominante. En este ensayo, discuto las implicaciones de estos problemas para la conducta e interpretación de la investigación.},
    número = {8},
}

