@article{Ioannidis2005False
    doi = {10.1371/journal.pmed.0020124},
    作者 = {Ioannidis, John P. A.}
    日记 = {PLOS Medicine}
    发布器 = {Public Library of Science}
    标题 = {Why Most Published Research Findings Are False}
    年 = {2005}，
    月 = {08}
    卷 = {2}
    url = {https://doi.org/10.1371/journal.pmed.0020124},
    pages = {null}
    摘要 = {摘要……人们越来越担心目前发表的大多数研究结果都是假的。 研究要求的确存在的概率可能取决于研究力量和偏见，取决于就同一问题进行的其他研究的数量。 而且重要的是，在每个科学领域探索的各种关系之间的真正关系与无关系之比。 在这一框架内，如果在一个领域进行的研究规模较小，研究结果就不太可能是真实的。 当效果大小较小时；当经过测试的关系出现更多和更少的预选时； (a) 在设计、定义、结果和分析方式方面有更大的灵活性； 当更多的小组参与科学领域的追查具有统计意义的活动时。 模拟显示，对于大多数研究的设计和设置来说，研究声称更有可能是虚假的，而不是真实的。 此外，在许多现有的科学领域，所称的研究结果往往只是衡量普遍偏向的准确尺度。 在本文中，我讨论这些问题对研究的进行和解释的影响。
    数字 = {8}
}

