@article{Ioannidis2005False،
    doi = {10.1371/journal.pmed.0020124}،
    auteur = {Ioannidis, John P. A.},
    مجلة = {PLOS Medicine}،
    éditeur = {Public Library of Science},
    titer = {Why Most Published Research Findings Are False},
    année = {2005},
    mois = {08},
    المجلد = {2}،
    url = {https://doi.org/10.1371/journal.pmed.0020124},
    الصفحات = {null}،
    abstract = {Summary Il est de plus en plus inquiétant que la plupart des résultats de recherche publiés soient faux. La probabilité qu'une allégation de recherche soit vraie peut dépendre du pouvoir et du biais d'étude, du nombre d'autres études sur la même question, et, surtout, le rapport entre le vrai et l'absence de relation entre les relations examinées dans chaque domaine scientifique. Dans ce cadre, il est moins probable que les études menées dans un domaine soient moins nombreuses ; lorsque la taille de l'effet est plus petite ; quand il y a un plus grand nombre et une moindre présélection des relations testées; là où il y a une plus grande flexibilité dans les conceptions, les définitions, les résultats et les modes d'analyse; quand il y a plus d'intérêt et d'autres préjugés financiers et autres; et quand plus d'équipes sont impliquées dans un champ scientifique dans la poursuite de l'importance statistique. Les simulations montrent que pour la plupart des conceptions et des contextes d'étude, il est plus probable qu'une recherche soit fausse que vraie. En outre, pour de nombreux domaines scientifiques actuels, les conclusions de la recherche prétendue peuvent souvent être simplement des mesures précises de la tendance dominante. Dans cet essai, je discute des implications de ces problèmes pour la conduite et l'interprétation de la recherche.},
    nombre = {8},
}

