@Artikel{Ioannidis2005False،
    doi = {10.1371/journal.pmed.0020124}،
    Autor = {Ioannidis, John P. A.},
    مجلة = {PLOS Medicine}،
    الناشر = {Public Library of Science},
    العنوان = {Why Most Published Research Findings Are False},
    jahr = {2005},
    Monat = {08},
    lautstärke = {2},
    url = {https://doi.org/10.1371/journal.pmed.0020124},
    Seiten = {null},
    abstract = {Zusammenfassung Es wächst die Sorge, dass die meisten aktuellen veröffentlichten Forschungsergebnisse falsch sind. Die Wahrscheinlichkeit, dass eine Forschungsangabe zutrifft, kann von der Studienmacht und der Voreingenommenheit abhängen, der Anzahl anderer Studien zu derselben Frage, und vor allem das Verhältnis von treu zu keiner Beziehung zwischen den Beziehungen in jedem wissenschaftlichen Bereich. In diesem Rahmen ist ein Forschungsergebnis weniger wahrscheinlich, wenn die auf einem Gebiet durchgeführten Studien kleiner sind; wenn Effektgrößen kleiner sind; wenn es eine größere Anzahl und geringere Vorauswahl der getesteten Beziehungen gibt; wo es eine größere Flexibilität bei Designs, Definitionen, Ergebnissen und analytischen Modalitäten gibt; wenn es größere finanzielle und andere Interessen und Vorurteile gibt; und wenn mehr Teams in einem wissenschaftlichen Bereich in der Jagd von statistischer Bedeutung beteiligt sind. Simulationen zeigen, dass es für die meisten Studienkonzepte und -einstellungen wahrscheinlicher ist, dass eine Forschungsangabe falsch ist als wahr. Darüber hinaus könnten für viele aktuelle wissenschaftliche Bereiche behauptete Forschungsergebnisse oft einfach nur genaue Messungen der vorherrschenden Voreingenommenheit sein. In diesem Essay bespreche ich die Auswirkungen dieser Probleme auf das Verhalten und die Interpretation der Forschung.},
    anzahl = {8},
}

